{
  "name": "ngx-stimulsoft",
  "version": "18.0.0",
  "author": "Sajad Mehrabi",
  "description": "Angular reporter for stimulsoftJS",
  "license": "MIT",
  "scripts": {
    "build": "ng build:lib",
    "build:lib": "ng build ngx-stimulsoft",
    "deploy": "npm run build:lib && npm publish --access=public dist",
    "lint": "eslint . --ext .js,.ts",
    "lint:fix": "npm run lint -- --fix",
    "ng": "ng",
    "prepare": "husky install",
    "prettier:check": "prettier --check \"{,src/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "prettier:format": "prettier --write \"{,src/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss}\"",
    "start": "ng serve",
    "test": "ng test"
  },
  "keywords": [
    "ngx",
    "ng",
    "nga",
    "avan",
    "angular",
    "stimulsoft",
    "reporter",
    "library",
    "ngx stimulsoft",
    "angular reporters",
    "avan reporter"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mehrabisajad/ngx-stimulsoft.git"
  },
  "dependencies": {
    "@angular/core": "~18.2.1",
    "@angular/common": "~18.2.1",
    "ngx-source": "13.0.2",
    "rxjs": "^7.8.2",
    "zone.js": "^0.15.1",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~18.2.1",
    "@angular-eslint/eslint-plugin": "18.3.0",
    "@angular/cli": "~18.2.1",
    "@angular/compiler": "~18.2.1",
    "@angular/compiler-cli": "~18.2.1",
    "@types/jasmine": "~5.1.8",
    "@types/node": "24.0.14",
    "@typescript-eslint/eslint-plugin": "8.37.0",
    "@typescript-eslint/parser": "8.37.0",
    "@typescript-eslint/utils": "8.37.0",
    "eslint": "9.31.0",
    "eslint-config-prettier": "10.1.5",
    "husky": "9.1.7",
    "jasmine-core": "5.8.0",
    "lint-staged": "16.1.2",
    "ng-packagr": "~18.2.1",
    "prettier": "3.6.2",
    "prettier-plugin-packagejson": "2.5.18",
    "typescript": "~5.5.4"
  },
  "resolutions": {
    "string-width": "4.1.0",
    "strip-ansi": "6.0.0",
    "wrap-ansi": "6.2.0"
  }
}
